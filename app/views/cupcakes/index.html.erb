
<div id='heading'>
<h1> Cupcake Selection: </h1>
</div>

<div id='products'>
    <% @cupcakes.each do |cupcake| %>
        <div class="tile">
							<h2> <%= link_to cupcake.name, cupcake_path(cupcake) %> </h2>
            <%= link_to image_tag cupcake.img_url  %>
        </div>
    <% end %>
</div>

